package Loop;

public class AlternatePrime {
	
	public static void test()
	{
		for(int i=1;i<=20;i++)
		{
			int count=0;
			for(int j=2;j<=i;j++)
			{
				if(i%j==0)
				{
					count=count+1;
				}
			}
			if(count==1)
			{
				for(int k=1;k<=20;k++)
				{
					if(i%2!=0) {
						System.out.print(i+" ");
					}
				
				}
			}
		}
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		test();
		

	}

}
