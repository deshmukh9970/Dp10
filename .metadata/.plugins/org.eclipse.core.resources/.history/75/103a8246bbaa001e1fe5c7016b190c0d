package Loop;

public class AlternatePrime {
	
	public static void test()
	{
		for(int i=1;i<=20;i++)
		{
			int count=0;
			for(int j=2;j<=i;j++)
			{
				if(i%j==0)
				{
					count=count+1;
				}
			}
			if(count==1)
			{
				System.out.print(i+" ");
				i=+2;
			}
		}
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		test();
		

	}

}
