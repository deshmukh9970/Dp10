package System.out;

public class Palindrome {
	
	static void checkPaindrom(int num)
	{
		int temp=num;
		int rev=0;
		int rem=0;
		
		while(num !=0)
		{
		   rev=num%10;
		   rem=rev*10+rem;
		   num=num/10;
			
		}
		
		if(temp==num)
		{
			System.out.println("is palindrom number ");
		}
		else 
		{
			System.out.println("Not paindrom Number");
		}
		
		
	}
	
	public static void main(String[] args) {
		 
		for(int i=0;i<=500;i++)
		{
			checkPaindrom(i);
		}
	}

}
