package com.string;

public class StringPalindndromWithOrWithoutTemp {
	
	static boolean checkPalideromWithTemp(String s)
	{
		String rev=" ";
		
		for(int i=s.length()-1;i>=0;i--)
		{
			rev=rev+s.charAt(i);
		}
		
		 if(rev.equalsIgnoreCase(s))
		 {
			 return true;
		 }else
		 {
			 return false;
		 }
	}

}
