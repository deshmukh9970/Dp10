package com.array2d;

public class Demo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		int row=3;
		int col=3;
		
		int arr[][]=new int[row][col];
		
		int no=1;
		
		for(int i=0;i<row;i++)
		{
			for(int j=0;j<col;j++)
			{
				arr[i][j]=no;
				no++;
			}
		}
		
		//here printing
		
		for(int i=0;i<row;i++)
		{
			for(int j=0;j<col;j++)
			{
				System.out.println(arr[i][j]);
			}
		}
		
		

	}

}
