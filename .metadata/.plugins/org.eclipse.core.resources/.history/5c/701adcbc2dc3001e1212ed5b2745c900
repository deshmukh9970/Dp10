package com.array;

public class TestArray {
	
	public static void sortArryStart(int arr[])
	{
		int count=0;
		int i;
		for(i=0;i<arr.length;i++)
		{
			
			if(arr[i] !=0)
			{
				arr[count]=arr[i];
				count++;
			}
		}
		while((count<arr.length))
		{
			arr[count]=arr[i];
			count++;
		}
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
